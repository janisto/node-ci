  "dependencies": {
    "express": "4.16.3",
    "moment": "2.22.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/coverage/",
      "/modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/modules/"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " â€º ",
