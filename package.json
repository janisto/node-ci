    "lint": "eslint ./",
    "test": "NODE_ENV=test jest",
    "audit": "auditjs -p ./package.json"
  },
  "devDependencies": {
    "auditjs": "2.2.6",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.10.3",
    "jest": "21.2.1",
